<script src="/libs/jquery18/jquery.min.js"></script>
<link rel="stylesheet" href="/app/css/distcss/camera.css"/>
<style>
    #camera_wrap_4 {
        height: 100%;
        margin-bottom: 0!important;
    }
    @media (max-width: 1440px) {
        .camera-container {
            height: 290px;
        }
    }
    @media (min-width: 1441px) and (max-width: 1600px) {
        .camera-container {
            height: 330px;
        }
    }
    @media (min-width: 1601px) {
        .camera-container {
            height: 350px;
        }
    }



    .camera_overlayer {
        opacity: .05;
    }
</style>
<script>
    $(function() {

        $('#camera_wrap_4').camera({
            height        : 'auto',
            loader        : 'bar',
            pagination    : false,
            //thumbnails    : true,
            hover         : false,
            opacityOnGrid : false,
            imagePath     : '../images/',
            pauseOnClick  : false
        });

    });
</script>
<script src="/libs/camera/jquery.easing.1.3.js"></script>
<script src="/libs/camera/jquery.mobile.customized.min.js"></script>
<script src="/libs/camera/camera.min.js"></script>